    status_0 = {'status': 0}
    try:
        ten_domain = check_share_domain()
        # ten_domain.check_ten_domain()
        domain_lists = ten_domain.get_ten_domain()
        print domain_lists
    except Exception, e:
        # print e
        return HttpResponse(json.dumps(status_0))
    else:
        return HttpResponse(json.dumps(domain_lists))