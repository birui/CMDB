 select model_name,count(domain_name) from  CMDB_coohua_share_domain where weixin_status = -1 and date_format(now(),'%Y/%m/%d %H:%M:%S') - DATE_FORMAT( deny_date, '%Y-%m-%d %H:%M:%S') <= 1  Group By model_name; 



 select * from tn where dt >= DATE_FORMAT(date_sub(current_date(),interval 1 day),'%Y-%m-%d 12:00:00') and dt <=DATE_FORMAT(now(),'%Y-%m-%d 12:00:00');


 select model_name,count(domain_name) from  CMDB_coohua_share_domain where weixin_status = -1 and deny_date = DATE_FORMAT(date_sub(current_date(),interval 1 day),'%Y-%m-%d') and deny_date <=DATE_FORMAT(now(),'%Y-%m-%d %H:%M:%S');